/**app.wxss**/
@import '/public/style/weui.wxss';
@import "/wxParse/wxParse.wxss";

page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}

.btnInfo{
    background: #4891D9;
    color:#fff;
}
.my_cells:before,.my_cells:after{
    border:0
}
.my_cells .my_cell:before,.my_cells .my_cell:after{
    border:0
}